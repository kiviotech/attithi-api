{
  "kind": "collectionType",
  "collectionName": "rooms",
  "info": {
    "singularName": "room",
    "pluralName": "rooms",
    "displayName": "room",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "room_number": {
      "type": "string"
    },
    "room_type": {
      "type": "enumeration",
      "enum": [
        "AC Rooms",
        "Non-AC Rooms"
      ]
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "available",
        "occupied",
        "cleaning",
        "blocked"
      ]
    },
    "beds": {
      "type": "integer"
    },
    "room_category": {
      "type": "enumeration",
      "enum": [
        "Guest house",
        "F",
        "Yatri Niwas"
      ]
    },
    "available_beds": {
      "type": "integer"
    },
    "guests": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::guest-detail.guest-detail",
      "mappedBy": "room"
    },
    "booking_requests": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::booking-request.booking-request",
      "mappedBy": "room"
    }
  }
}
