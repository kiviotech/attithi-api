{
  "kind": "collectionType",
  "collectionName": "guest_details",
  "info": {
    "singularName": "guest-detail",
    "pluralName": "guest-details",
    "displayName": "Guest",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "phone_number": {
      "type": "string"
    },
    "occupation": {
      "type": "string"
    },
    "address": {
      "type": "text"
    },
    "donations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::donation.donation",
      "mappedBy": "guest"
    },
    "booking_request": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::booking-request.booking-request",
      "inversedBy": "guests"
    },
    "age": {
      "type": "integer"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "M",
        "F",
        "Other"
      ]
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "approved",
        "pending",
        "rejected",
        "none"
      ]
    },
    "relationship": {
      "type": "enumeration",
      "enum": [
        "mother",
        "father",
        "son",
        "daughter",
        "wife",
        "aunt",
        "friend",
        "applicant",
        "other"
      ]
    },
    "email": {
      "type": "email"
    },
    "room": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::room.room",
      "inversedBy": "guests"
    },
    "arrival_date": {
      "type": "date"
    },
    "departure_date": {
      "type": "date"
    },
    "deeksha": {
      "type": "string"
    },
    "identity_proof": {
      "type": "string"
    },
    "identity_number": {
      "type": "string"
    }
  }
}
